{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CurrencyCacheSettings": {
    "Directory": ".\\Cache\\CurrencyCache",
    "ExpirationTimeInHours": "2"
  },
  "ApiSettings": {
    "ApiKey": "YOUR_API_KEY",
    "DefaultCurrency": "RUB",
    "BaseUrl": "https://api.currencyapi.com/v3"
  },
  "GrpcPort": "5167",
  "ConnectionStrings": {
    "currency_api": "Host=localhost;Port=5433;Database=currency_api;Username=user;Password=pass"
  }
}
